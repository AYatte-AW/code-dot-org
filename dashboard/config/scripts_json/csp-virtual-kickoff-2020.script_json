    {
      "key": "d635e321-a932-4c6c-8e86-9348d22760ff",
      "position": 5,
      "properties": {
        "description": "* Show your video during our workshop\r\n* Mute yourself in a large group\r\n* Be present\r\n* Start/end on time\r\n* Monitor your air time\r\n* Contribute to our cohort community"
      },
      "seeding_key": {
        "activity_section.key": "d635e321-a932-4c6c-8e86-9348d22760ff",
