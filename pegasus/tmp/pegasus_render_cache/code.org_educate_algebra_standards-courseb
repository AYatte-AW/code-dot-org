<script type="text/javascript">
    var vars = [], hash;
        var q = document.URL.split('?')[1];
        if(q != undefined){
            q = q.split('&');
            for(var i = 0; i < q.length; i++){
                hash = q[i].split('=');
                vars.push(hash[1]);
                vars[hash[0]] = hash[1];
            }
    }

    if (vars['pdf']) {
        jQuery(function () {
            $('a[data-pdf-link]').each(function () {
                this.href = $(this).attr('data-pdf-link');
            });
        });
    }
</script>







