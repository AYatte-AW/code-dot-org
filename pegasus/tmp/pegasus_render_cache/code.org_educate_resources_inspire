<style>
  .modal{
    text-align: center;
  }
</style>

<script>
  window["showVideo_cs_changing_everything"] = function()
  {
    $("#showVideo_cs_changing_everything").modal('show');
    setupPopupWindows();
    window["playVideo_cs_changing_everything"](true);
  
    // send video start action to Google Analytics. Use action field to indicate whether this was from YouTube or Code.org CloudFront
    var fallbackVideo = $("#showVideo_cs_changing_everything video");
    var eventAction = (fallbackVideo.length > 0 ? 'startVideoFallback' : 'startVideoYouTube');
    ga('send', 'event', 'showvideo', eventAction, 'cs_changing_everything');
  };
  
  $(document).ready(function() {
    $("#showVideo_cs_changing_everything").on('hidden.bs.modal', function ()
    {
      // Pause the video.
      window["playVideo_cs_changing_everything"](false);
    });
  });
  
  window["playVideo_cs_changing_everything"] = function(play)
  {
    var fallbackVideo = $("#showVideo_cs_changing_everything video");
  
    var youtubeIframeContent = null;
    var videoDiv = $("#showVideo_cs_changing_everything .videodiv");
    if (videoDiv.length > 0)
    {
      var iframe = videoDiv[0].getElementsByTagName("iframe");
      if (iframe.length > 0)
      {
        youtubeIframeContent = iframe[0].contentWindow;
      }
    }
  
    if (fallbackVideo.length > 0)
    {
      videojs(fallbackVideo[0], {}, function() {
        play ? this.play() : this.pause();
      });
    }
    else if (youtubeIframeContent)
    {
      func = play ? 'playVideo' : 'pauseVideo';
      youtubeIframeContent.postMessage('{"event":"command","func":"' + func + '","args":""}', '*');
    }
  }
</script>

<p></p>
</div>

