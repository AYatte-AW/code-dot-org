    }
  ];
    var id = '#stats-carousel';
  
    // Define the carousel using the ID.
    $(id + ' .slides').carouFredSel({
      items :  {
        height : 'variable'
      },
      auto  : false,
      responsive : true,
      width : '100%',
      height : 'variable',
      prev  : id + ' .slide-prev',
      next  : id + ' .slide-next',
      pagination : '#stats-carousel-pagination',
      swipe : {
        onTouch : true,
        onMouse : false
      }
    }).trigger(resize);
  });
</script>

